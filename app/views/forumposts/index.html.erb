<h1>Listing forumposts</h1>

<table>
  <tr>
    <th>Content</th>
    <th>User</th>
    <th>Topic</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @forumposts.each do |forumpost| %>
  <tr>
    <td><%= forumpost.content %></td>
    <td><%= forumpost.user_id %></td>
    <td><%= forumpost.topic_id %></td>
    <td><%= link_to 'Show', forumpost %></td>
    <td><%= link_to 'Edit', edit_forumpost_path(forumpost) %></td>
    <td><%= link_to 'Destroy', forumpost, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

